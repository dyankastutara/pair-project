<!DOCTYPE html>
<html>
  <head>
    <title>nanyain.com</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <p><a href="question/add">Add Question</a></p>
    <fieldset>
      <table width="100%">
        <tr align="left">
          <th width="5%">No</th>
          <th width="15%">User</th>
          <th width="50%">Question</th>
          <th width="30%">Action</th>
        </tr>
        <% let i=1; list.forEach((row)=>{%>
        <tr>
          <td><%= i++ %></td>
          <td><%= row.User.name%></td>
          <td><%= row.question %></td>

          <td><a href="/tags/add/<%=row.id%>">Add Tags</a> | <a href="/answer/add_answer/<%=row.id%>">Answer</a> | <a href="/question/view/<%=row.id%>">View</a> | <a href="/question/delete/<%=row.id%>">Delete</a> | <a href="/question/edit/<%=row.id%>">Edit</a></td>

        </tr>
        <% }) %>
      </table>
    </fieldset>
  </body>
</html>
